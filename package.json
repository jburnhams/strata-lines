{
  "name": "stratalines",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "npm run copy-wasm && vite",
    "build": "npm run copy-wasm && tsc && vite build && node scripts/inject-build-timestamp.mjs && node scripts/verify-wasm-in-build.mjs",
    "preview": "vite preview",
    "copy-wasm": "mkdir -p public/assets && cp node_modules/image-stitch/dist/bundles/jpeg_encoder_bg.wasm public/assets/",
    "test": "NODE_OPTIONS='--max-old-space-size=2048' jest --maxWorkers=1",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=2048' jest --selectProjects unit --maxWorkers=1",
    "test:integration": "NODE_OPTIONS='--max-old-space-size=2048' jest --selectProjects integration --maxWorkers=1",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=2048' jest --selectProjects unit --coverage --maxWorkers=1",
    "test:coverage:full": "NODE_OPTIONS='--max-old-space-size=2048' jest --coverage --maxWorkers=1",
    "test:watch": "jest --watch --maxWorkers=1",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@garmin/fitsdk": "^21.115.0",
    "@we-gold/gpxjs": "^1.1.0",
    "html2canvas": "^1.4.1",
    "image-stitch": "^1.1.50",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "pako": "^2.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "xmldom-qsa": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.9.2",
    "@types/pako": "^2.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.1",
    "leaflet-node": "^2.0.24",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.3",
    "vite": "^7.1.12"
  }
}
